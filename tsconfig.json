{
    "compilerOptions": {
      "target": "es2022",
      "module": "esnext",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "outDir": "dist/models",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["./model/**/*"],
    "exclude": ["node_modules"]
  }